{
  "name": "@jsonstore/sdk",
  "version": "0.1.0",
  "description": "Core SDK for JSON Store - Git-backed, file-based data store",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "NODE_OPTIONS=--expose-gc tsx benchmarks/performance.bench.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "json",
    "database",
    "git",
    "mango",
    "query"
  ],
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
